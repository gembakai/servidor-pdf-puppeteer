<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Reporte</title>
    <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
    <h1>Reporte Dinámico</h1>
    <table>
        <tr>    
            <th>ID</th>
            <th>Nombre</th>
            <th>Descripción</th>
        </tr>
        <% data.items.forEach(item => { %>
            <tr>
                <td><%= item.id %></td>
                <td><%= item.nombre %></td>
                <td><%= item.descripcion %></td>
            </tr>
        <% }); %>
    </table>
</body>
</html>
